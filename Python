def rev_list(nums):
    list_rev=[]
    for i in nums:
        list_rev.append(nums[-i])
    print(list_rev)   

rev_list(list_nums)



def rev_list(nums):
  return nums[::-1]
print(rev_list(list_nums))


def even_sum(nums):
    even_nums=[]
    len_nums=len(nums)
    for i in range(len_nums):
        if nums[i]%2==0:
            even_nums.append(nums[i])
    return sum(even_nums)

even_sum(list_nums)


even_sum = lambda nums: sum(num for num in nums if num % 2 == 0)
even_sum(list_nums)


